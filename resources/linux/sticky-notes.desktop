[Desktop Entry]
Name=Sticky Notes
Comment=A minimal, beautiful sticky notes app for Ubuntu with matte dark theme and cyan accents
Exec=env ELECTRON_IS_DEV=0 /usr/local/bin/sticky-notes --no-sandbox %U
Icon=sticky-notes-electron
Terminal=false
Type=Application
StartupWMClass=sticky-notes-electron
StartupNotify=true
Keywords=notes;todo;sticky;desktop;
Categories=Utility;Office;
MimeType=text/plain;
Actions=NewNote;

[Desktop Action NewNote]
Name=New Note
Exec=env ELECTRON_IS_DEV=0 /usr/local/bin/sticky-notes --new-note --no-sandbox